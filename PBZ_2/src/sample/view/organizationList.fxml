<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.OrganizationListController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane fx:id="mainPane" prefHeight="326.0" prefWidth="684.0">
         <children>
            <TableView fx:id="organizationTableView" layoutX="25.0" layoutY="34.0" prefHeight="200.0" prefWidth="649.0">
              <columns>
                <TableColumn fx:id="idColumn" prefWidth="34.0" text="id" />
                <TableColumn fx:id="fullnameColumn" prefWidth="147.0" text="полное название" />
                  <TableColumn fx:id="shortnameColumn" prefWidth="123.0" text="краткое название" />
                  <TableColumn fx:id="addressColumn" prefWidth="106.0" text="адресс" />
                  <TableColumn fx:id="bankColumn" prefWidth="121.0" text="номер банка" />
                  <TableColumn fx:id="specialtyColumn" prefWidth="117.0" text="специальность" />
              </columns>
            </TableView>
            <Button fx:id="addButton" layoutX="25.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Добавить" />
            <Button fx:id="deleteButton" layoutX="307.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Удалить" />
            <Button fx:id="editButton" layoutX="569.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="105.0" text="Редактировать" />
            <Button fx:id="cancelButton" layoutX="316.0" layoutY="287.0" mnemonicParsing="false" text="Назад" />
         </children></Pane>
   </children>
</GridPane>

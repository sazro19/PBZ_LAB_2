<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.StaffListController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane fx:id="mainPane" prefHeight="326.0" prefWidth="564.0">
         <children>
            <TableView fx:id="staffTableView" layoutX="61.0" layoutY="34.0" prefHeight="200.0" prefWidth="373.0">
              <columns>
                <TableColumn fx:id="fullnameColumn" prefWidth="101.0" text="ФИО" />
                <TableColumn fx:id="ageColumn" prefWidth="180.0" text="Возраст" />
                  <TableColumn fx:id="riskCategoryColumn" prefWidth="91.0" text="Категория" />
              </columns>
            </TableView>
            <Button fx:id="addButton" layoutX="61.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="110.0" text="Добавить" />
            <Button fx:id="deleteButton" layoutX="193.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="110.0" text="Удалить" />
            <Button fx:id="editButton" layoutX="324.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="110.0" text="Редактировать" />
            <Button fx:id="cancelButton" layoutX="222.0" layoutY="287.0" mnemonicParsing="false" text="Назад" />
         </children></Pane>
   </children>
</GridPane>

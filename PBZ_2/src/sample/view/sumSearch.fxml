            <TableView fx:id="resultTableView" layoutX="325.0" layoutY="12.0" prefHeight="200.0" prefWidth="272.0">
              <columns>
                <TableColumn fx:id="startDateColumn" prefWidth="132.0" text="дата заключеня" />
